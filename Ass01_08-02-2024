student={}
l=[]
s1=int(input('Enter S1'))
s2=int(input('Enter S2'))
s3=int(input('Enter S3'))
s4=int(input('Enter S4'))
s5=int(input('Enter S5'))
l.append(s1)
l.append(s2)
l.append(s3)
l.append(s4)
l.append(s5)
total=0
for i in range(1,6,+1):
    student[i]=l[i-1]
    total=total+student[i]
avg=total//5
print('Total:',total)
print('Percentage:',avg)
if avg>60:
    print('Pass')
else:
    print('Fail')
